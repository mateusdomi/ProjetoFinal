@{
    if (TempData["Info"] != null)
    {
        <div class="alert alert-info">
            <p> @TempData["Info"]</p>
        </div>
        
    }
    else if (TempData["Warn"] != null)
    {
        <div class="alert alert-warning">
            <p> @TempData["Warn"]</p>
        </div>
    }
    else if (TempData["Error"] != null)
    {
        <div class="alert alert-danger">
            <p> @TempData["Error"]</p>
        </div>
    }

}
@{
    TempData["Info"]= null;
    TempData["Warn"]= null;
    TempData["Error"]= null;
}